<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kyrie.mapper.ProductMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kyrie.pojo.Product">
        <result column="name" property="name" />
        <result column="fivePoint" property="fivePoint" />
        <result column="description" property="description" />
        <result column="asin" property="asin" />
        <result column="sku" property="sku" />
        <result column="createDate" property="createDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        name, fivePoint, description, asin, sku, createDate
    </sql>

</mapper>
